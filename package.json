{
  "name": "my-package",
  "license": "MIT",
  "description": "A cli tool for altron rich text editor.",
  "keywords": [
    "altron",
    "cli",
    "typescript",
    "javascript",
    "svelte"
  ],
  "author": "Aymen keskas",
  "bin": {
    "altron-cli": "./dist/index.js"
  },
  "maintainers": [
    "Aymen keskas <keskasaymen8@gmail.com> (https://empty-void.vercel.app/)"
  ],
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "./dist/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "settings": {
    "autoInstallPeers": false
  },
  "scripts": {
    "dev": "tsup > /dev/null && node dist/index.js",
    "start": "node dist/index.js",
    "build": "tsup",
    "lint": "tsc",
    "release": "tsup && changeset publish"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.7",
    "tsup": "^6.5.0",
    "type-fest": "^4.10.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/semver": "^7.5.6",
    "cfonts": "^3.2.0",
    "chalk": "^5.3.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "semver": "^7.5.4",
    "svelte-select": "^5.8.3"
  }
}
